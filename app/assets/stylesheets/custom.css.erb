* { padding: 0; margin: 0px; box-sizing: border-box; }
body { height: 2000px; }
header {
	padding: 0px;
	color: #FFF;
  height: 30vh;
  min-height: 15rem;
	text-shadow: 1px 1px 1px #333;
  color: white;
  background-image: url(<%= asset_path 'kn-header.jpg' %>) !important;
  background-repeat: no-repeat;
  background-size: cover;
}

footer {
	background-image: linear-gradient(to right, #D276A3 , #D43D65);
	text-align: center;
	width: 100%;
	height: 30vh;
  min-height: 10vh;
	background-size: cover;
	background-attachment: fixed;
	position: relative;
	overflow: hidden;
	border-radius: 85% 85% 0 0 / 30%;
}
footer .overlay{
	width: 100%;
	height: 100%;
	padding: 1em;
	color: #FFF;
	text-shadow: 1px 1px 1px #333;
}

.table_header {
  font-weight: bold;
  background-color: rgb(191, 191, 191);
}

.social-box {
  margin: 0 auto;
  width: 400px;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
}

.social-box img {
  width: 35px;
  margin-right: 5px;
}

.info_section {
  color: #ffffff;
  font-family: 'Poppins', sans-serif;
}

.info_section h5 {
  margin-bottom: 12px;
  font-size: 26px;
}

.info_logo-box {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
}

.info_logo-box h2 {
  font-weight: bold;
  padding: 5px 35px;
  border-bottom: 1.5px solid #8547ca;
}

.info_section ul {
  padding: 0;
}

.info_section ul li {
  list-style-type: none;
  margin: 3px 0;
}

.info_section ul li a {
  color: #ffffff;
}

.info_section ul li a:hover {
  color: #8547ca;
}

.info_section .form_container input {
  width: 225px;
  height: 40px;
  padding: 10px;
}

.info_section .form_container button {
  border: none;
  outline: none;
  padding: 8px 30px;
  margin-top: 15px;
  font-size: 15px;
  text-transform: uppercase;
}

/* footer section*/
.footer_section {
  padding: 10px;
}

.footer_section p {
  margin: 0;
  text-align: center;
  color: #fefdfc;
}

.footer_section a {
  color: #fefdfc;
}
