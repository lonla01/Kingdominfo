#flag_fr div {
  padding 20px;
  vertical-align: middle;
  display: inline-block;
}

#flag_en div {
  padding 20px;
  vertical-align: middle;
  display: inline-block;
}

#icon_fr {
  display: inline-block;
  width: 42px;
  height: 24px;
  vertical-align: middle;
  background-image: url(<%= asset_path 'Flag_of_France.jpg' %>);
  background-repeat: no-repeat;
  background-size: cover;
}

#icon_en {
  width: 42px;
  height: 24px;
  vertical-align: middle;
  background-image: url(<%= asset_path 'Flag_of_USA.jpg' %>);
  background-repeat: no-repeat;
  background-size: cover;
}

h1 {
	font-family: 'Dancing Script', cursive;
	font-size: 80px;
	margin-bottom: 30px;
}

